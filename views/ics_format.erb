<%# coding: utf-8 %>BEGIN:VCALENDAR
PRODID:-//gam0022//TwinCal 2.0//EN
VERSION:2.0
CALSCALE:GREGORIAN
METHOD:PUBLISH
X-WR-CALNAME:筑波大学 授業時間割
X-WR-TIMEZONE:Asia/Tokyo
X-WR-CALDESC:筑波大学 授業時間割
BEGIN:VTIMEZONE
TZID:Asia/Tokyo
X-LIC-LOCATION:Asia/Tokyo
BEGIN:STANDARD
TZOFFSETFROM:+0900
TZOFFSETTO:+0900
TZNAME:JST
DTSTART:19700101T000000
END:STANDARD
END:VTIMEZONE
<% @subjects.each do |item| %>BEGIN:VEVENT
DTSTART;TZID=Asia/Tokyo:<%= item[:start] %>
DTEND;TZID=Asia/Tokyo:<%= item[:end]  %>
RRULE:FREQ=WEEKLY;UNTIL=<%= item[:until] %>;BYDAY=<%= WEEK[item[:wday].to_i][0..1] %>
DTSTAMP:<%= @DTSTAMP %>
CREATED:<%= @CREATED %>
DESCRIPTION:<%= "#{item[:code]} / #{item[:tani]}単位 / #{item[:teacher]}" %>
LAST-MODIFIED:<%= @LAST_MODIFIED %>
LOCATION:<%= item[:location] %>
SEQUENCE:0
STATUS:CONFIRMED
SUMMARY:<%= item[:name] %>
TRANSP:OPAQUE
END:VEVENT
<% end %>END:VCALENDAR
